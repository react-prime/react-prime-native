    "@react-navigation/stack": "6.1.1",
    "cross-fetch": "3.1.5",
    "qs": "6.10.3",
    "react": "17.0.2",
    "react-native": "0.67.3",
    "react-native-bootsplash": "4.1.3",
    "react-native-config": "1.4.5",
    "react-native-device-info": "8.4.9",
