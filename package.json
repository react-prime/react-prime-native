    "@react-navigation/stack": "6.0.9",
    "cross-fetch": "3.1.4",
    "moti": "0.16.0",
    "qs": "6.10.1",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-bootsplash": "3.2.6",
    "react-native-config": "1.4.4",
    "react-native-device-info": "8.3.3",
