{
  "name": "reactprimenative",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "lint": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts src/",
    "typecheck": "tsc",
    "ios": "react-native run-ios --scheme \"reactprimenative - Test\" --configuration \"Test - Debug\" --simulator=\"iPhone 11\"",
