{
  "name": "reactprimenative",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "lint": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts src/",
    "typecheck": "tsc --noEmit",
    "ios": "react-native run-ios --scheme \"reactprimenative - Test\" --configuration \"Test - Debug\"",
    "android": "ENVFILE=.env.dev react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android:release": "ENVFILE=.env.dev react-native run-android --variant=devRelease --appIdSuffix=dev",
    "postinstall": "npx jetify"
  },
