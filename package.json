    "@babel/runtime": "7.17.2",
    "@react-navigation/devtools": "6.0.5",
    "@types/react": "17.0.39",
    "@types/react-native": "0.66.16",
    "@types/styled-components": "5.1.23",
    "@typescript-eslint/eslint-plugin": "5.12.1",
